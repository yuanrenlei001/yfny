.banner {width: 100%;height:270rpx;}
page {background-color: #f2f2f2;}
.banner image {width: 100%;height:100%;}
.main {margin-top: 20rpx;border-top: 1px solid #dadada;padding: 0 60rpx;background: #fff;}
.list {border-bottom:   1px solid #dadada;padding: 35rpx 0;position: relative;}
.list:last-child {border-bottom: transparent;}
.listText01 {
  white-space:nowrap; 
text-overflow:ellipsis; 
-o-text-overflow:ellipsis; 
overflow: hidden;
font-size: 30rpx;color: #353535;
font-weight: bold;
}
.listText02 {position: relative;}
.listText03 {font-size: 24rpx;color: #b0b0b0;}
.listText04 {position: absolute;bottom:0;right:60rpx;font-size: 24rpx;color: #b0b0b0;}