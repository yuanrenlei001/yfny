page {background: #fff;}
.main {position: relative;}
.tab {position: relative;width:100%;height:100rpx;text-align: center;line-height: 100rpx;font-size: 30rpx;color: #a9a9a9;}
.tab view {width: 33.33%;float:left;border-bottom: 1px solid #e3e3e3;}
.tab .active {border-bottom: 1px solid #2faa22;color: #2faa22;}
.body {position: relative;padding: 0 60rpx;}
.img {width: 100%;text-align: center;margin-top: 15rpx;}
.img image {width: 660rpx;height:130rpx;}
.list {position: relative;width:100%;height:102rpx;padding-top: 34rpx;border-bottom: 1px solid #e0e0e0;}
.title {
  width:95%;
  font-size: 30rpx;
  color: #484b51; 
white-space:nowrap; 
text-overflow:ellipsis; 
-o-text-overflow:ellipsis; 
overflow: hidden;
}
.time {font-size: 24rpx;color: #b0b0b0;}
.num {position: absolute;right:0;bottom:28rpx;}
.num image {width:30rpx;height:23rpx;vertical-align: middle;}
.num view {float:left;margin: 0 10rpx;}
.tiwen {width:100%;height:98rpx;background: #61b343;text-align: center;font-size: 30rpx;color: #fff;line-height:100rpx;
  position: fixed;
  left:0;
  bottom:0;
}